# Ignore node_modules folder
node_modules/

# Ignore build output directories
dist/
build/

# Ignore environment files
.env
.env.local
.env.development
.env.test
.env.production
.env.*

# Ignore package lock and yarn lock
package-lock.json
yarn.lock

# Ignore logs
*.log
logs/
npm-debug.log*
yarn-debug.log*
yarn-error.log*

# Ignore coverage output (for tests)
coverage/

# Ignore configuration files for editors
.vscode/
.idea/
*.sublime-project
*.sublime-workspace

# Ignore other misc files
*.DS_Store
*.lock

# Ignore generated typescript declaration files
*.d.ts

# Ignore prettier ignore itself (optional)
.prettierignore
